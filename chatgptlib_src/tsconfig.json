{
  "compilerOptions": {
    "outDir": "../chatgptlib",
    "target": "es2017",
    "module": "commonjs",
    "allowJs": true,
    "alwaysStrict": false,
    "declaration": false,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "pretty": true,
    "removeComments": true,
    "strict": false,
    "lib": ["es5", "es6", "es2017"],
    "types": ["node"]
  },
  "exclude": ["node_modules", "dist"]
}
